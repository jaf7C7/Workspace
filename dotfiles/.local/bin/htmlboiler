#!/bin/sh
#
# html boilerplate

main()
{
	cat <<EOF
<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">

		<title>X</title>

		<link rel="stylesheet" href="X" type="text/css">
		<script defer src="X"></script>
	</head>
	<body>
	</body>
</html>
EOF
}

main "$@"

