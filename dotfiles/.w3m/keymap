keymap	C-t	NULL
keymap	C-u	NULL
keymap	C-v	NULL
keymap	C-w	NULL
keymap	C-x	NULL
keymap	C-y	NULL
keymap	C-z	SUSPEND
keymap	C-_	NULL
keymap	C-@	NULL

keymap	M-a	NULL
